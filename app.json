{
  "expo": {
    "name": "HashrobotMobile",
    "slug": "HashrobotMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "splash": {
      "image": "./assets/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#9DA73E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions" : ["WRITE_EXTERNAL_STORAGE", "VIBRATE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
